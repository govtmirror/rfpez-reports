extends ../layout

block content
  h3 Results for #{dataset_name}
  a(href="/datasets/#{dataset_name}.csv") Download as CSV

  table.table.table-bordered
    thead
      tr
        for key in keys
          th #{key}
        th created_at
    for datum in data
      tr
        for key in keys
          td #{datum.data[key]}
        td #{datum.created_at}